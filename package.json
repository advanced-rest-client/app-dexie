{
  "name": "app-dexie",
  "version": "1.0.3",
  "license": "LICENSE.md",
  "description": "A database wrapper for ARC",
  "devDependencies": {
    "polymer": "^0.5.0",
    "web-component-tester": "^4.3.4"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "app-dexie.html"
  ],
  "author": "Pawel Psztyc, The ARC team",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint app-dexie.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"app-dexie:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/app-dexie.git"
  }
}
